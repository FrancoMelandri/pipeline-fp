<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<RootNamespace>PipelineFp</RootNamespace>
		<IsPackable>true</IsPackable>
		<PackageDescription>Pipeline functional programming pattern</PackageDescription>
		<RepositoryUrl>https://github.com/FrancoMelandri/pipeline-fp.git</RepositoryUrl>
		<PackageReleaseNotes>pipeline-fp</PackageReleaseNotes>
		<PackageTags>Functional Programming</PackageTags>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="tiny-fp" Version="2.0.0" />
	</ItemGroup>

</Project>
